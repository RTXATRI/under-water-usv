<launch>
	<node name="imu" pkg="mz_usv" type="imu" output="screen">
	</node>
	<node name="imu_filter_node" pkg="imu_filter_madgwick" type="imu_filter_node" output="screen">
		<param name = "constant_dt"  value ="0.0"/>	
		<param name = "fixed_frame"  value ="base_link"/>
		<param name = "world_frame"  value ="ned"/>	
		<param name = "use_mag"  value ="true"/>	
		<param name = "use_magnetic_field_msg"  value ="true"/>
		<remap from="imu/data" to="/sensor/m_imu"/>
		<param name = "publish_debug_topics"  value ="true"/>
	</node>
</launch>
